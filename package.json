{
  "name": "TemplateAd",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 8'",
    "start": "react-native start",
    "test": "jest",
    "i": "--ignore-engines",
    "link_assets": "npx react-native-asset",
    "rename": "npx react-native-rename ",
    "apk": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "aab": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "reverse": "adb reverse tcp:9090 tcp:9090; adb reverse tcp:19001 tcp:19001;",
    "pod": "cd ios && pod install && cd ..",
    "createAppCenterAndroid": "appcenter apps create -d TemplateAd -o Android -p React-Native",
    "createAppCenterIOS": "appcenter apps create -d TemplateAd -o iOS -p React-Native",
    "codePushStagging": "appcenter codepush release-react -a cao.tran-adamosoft.com/TemplateAd-1 -d Staging",
    "codePushRelease": "appcenter codepush release-react -a cao.tran-adamosoft.com/TemplateAd-1 -d Production",
    "codePushReleaseAndroid": "appcenter codepush release-react -a cao.tran-adamosoft.com/TemplateAd -d Production",
    "codePushStagingAndroid": "appcenter codepush release-react -a cao.tran-adamosoft.com/TemplateAd -d Staging",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "preci": "rm -fr node_modules",
    "ci": "yarn install --frozen-lockfile",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/netinfo": "^9.3.7",
    "@react-native-firebase/app": "^17.2.0",
    "@react-native-firebase/firestore": "^17.2.0",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/material-bottom-tabs": "^6.2.15",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "@reduxjs/toolkit": "^1.9.3",
    "@shopify/flash-list": "^1.4.1",
    "@shopify/restyle": "^2.4.0",
    "@testing-library/react-native": "^11.5.0",
    "ahooks": "^3.7.4",
    "appcenter": "4.4.5",
    "appcenter-analytics": "4.4.5",
    "appcenter-crashes": "4.4.5",
    "axios": "^1.3.4",
    "babel-plugin-module-resolver": "^5.0.0",
    "dayjs": "^1.11.7",
    "formik": "^2.2.9",
    "i18next": "^22.4.10",
    "i18next-http-backend": "^2.1.1",
    "lodash": "^4.17.21",
    "patch-package": "^6.5.1",
    "postinstall-postinstall": "^2.1.0",
    "react": "18.2.0",
    "react-i18next": "^12.2.0",
    "react-native": "0.71.3",
    "react-native-bootsplash": "^4.5.2",
    "react-native-chart-kit": "^6.12.0",
    "react-native-code-push": "^7.1.0",
    "react-native-config": "^1.4.6",
    "react-native-device-info": "^10.2.0",
    "react-native-fast-image": "^8.6.1",
    "react-native-flash-message": "^0.4.0",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-mask-input": "^1.2.3",
    "react-native-mmkv": "^2.7.0",
    "react-native-modal": "^13.0.1",
    "react-native-month-year-picker": "^1.9.0",
    "react-native-reanimated": "^3.0.0",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-svg": "^13.8.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-text-ticker": "^1.14.0",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.2",
    "reactotron-react-native": "^5.0.2",
    "reactotron-redux": "^3.1.3",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "yarn": "^1.22.19",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.70.2",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.23.1",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "prettier src --write",
      "eslint src --fix",
      "git add"
    ]
  }
}
